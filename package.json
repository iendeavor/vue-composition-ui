    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "4.0.5",
    "vite": "^1.0.0-rc.9",
    "vue": "^3.0.2"
  },
  "peerDependencies": {
    "vue": "^3.0.2"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "docs": "api-documenter markdown -i temp -o docs",
