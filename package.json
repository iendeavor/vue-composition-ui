  "types": "dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iendeavor/vue-composition-ui.git"
  },
  "bugs": {
    "url": "https://github.com/iendeavor/vue-composition-ui/issues"
  },
  "homepage": "https://iendeavor.github.io/vue-composition-ui",
